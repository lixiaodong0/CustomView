<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.lixd.costom.view">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".recyclerview.decoration.logistics.LogisticsDecorationActivity">
        </activity>
        <activity android:name=".recyclerview.decoration.base.BaseDecorationActivity">
        </activity>
        <activity android:name=".recyclerview.RecyclerViewDemoActivity">
        </activity>
        <activity android:name=".recyclerview.decoration.ItemDecorationDemoActivity">
        </activity>
        <activity android:name=".flowlayout.CustomFlowLayoutActivity">
        </activity>
        <activity android:name=".linearlayout.CustomLinearLayoutActivity">
        </activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>